{
  "name": "spiral-packages/bootloaders-discover",
  "description": "Bootloaders discoverer for Spiral Framework",
  "keywords": [
    "spiral-packages",
    "spiral",
    "bootloaders-discover"
  ],
  "homepage": "https://github.com/spiral-packages/bootloaders-discover",
  "license": "MIT",
  "authors": [
    {
      "name": "butschster",
      "email": "butschster@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "spiral/boot": "^2.9",
    "spiral/files": "^2.9"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^9.5",
    "spiral/testing": "^1.0",
    "vimeo/psalm": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\BootloadersDiscover\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\BootloadersDiscover\\Tests\\App\\": "tests/app",
      "Spiral\\BootloadersDiscover\\Tests\\": "tests/src"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm --config=psalm.xml ./src"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
